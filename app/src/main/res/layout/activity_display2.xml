                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:onClick="onRadioButtonClickedD2"
                    android:text="alle"
                    android:textColor="#FFFFFFFF" />
            </RadioGroup>

            <TextView
                android:id="@+id/platz2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:textColor="#FFFFFFFF"
                android:text="          " />

